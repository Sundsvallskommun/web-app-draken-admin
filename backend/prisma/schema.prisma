generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/database/database.db"
}

model FeatureFlags {
  id             Int      @id @default(autoincrement())
  name           String
  value          String?
  enabled        Boolean  @default(false)
  application    String
  namespace      String
  municipalityId Int
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}